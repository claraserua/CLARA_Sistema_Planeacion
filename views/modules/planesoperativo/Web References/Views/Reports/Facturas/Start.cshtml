
@{
    ViewBag.Title = "Start";
}

<!-- begin row Formulario -->
<div class="row" id="frm-importar">
    <!-- begin col-12 -->
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">

                    <a data-click="panel-collapse" class="btn btn-xs btn-icon btn-circle btn-warning" href="javascript:;"><i class="fa fa-minus"></i></a>

                </div>
                <h4 class="panel-title">Facturas</h4>
            </div>

            <div class="panel-body">             

            
                    <div class="tab-pane fade active in" id="default-tab-1">

                        <form action="/" method="POST">
                            <fieldset>                              

                                <div class="row">

                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label for="fechai">Fecha de Pago (Inicio) *</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" id="fechai">
                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label for="fechaf">Fecha de Pago (Fin) *</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" id="fechaf">
                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                            </div>
                                        </div>
                                    </div>



                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label for="cve_tipodepago">Tipo de pago *</label>
                                            <select id="cve_tipodepago" class="form-control">
                                                                                   
                                             
                                           
                                                <option value="H">
                                                    Honorarios  <!-- option -->                                                 
                                               </option>

                                                <option value="F">
                                                    Facturas <!-- option -->
                                                </option>

                                                <option value="A">
                                                    Asimilados <!-- option -->
                                                </option>
                                            </select>
                                        </div>
                                    </div>

                                    <input type="hidden" class="form-control" id="publicar" value="">


                                </div>
                                <br />


                                <div class="row right">

                                    <!--<button type="button" id="formbtn_publicar" class="btn btn-sm btn-primary" onclick="formPage.btnpublicar_selec_todos()">Publicar</button>-->
                                    <!-- <button type="button" id="formbtn_despublicar" class="btn btn-sm btn-primary" onclick="formPage.btndespublicar_selec_todos();">DesPublicar</button>-->
                                     <button type="button" id="formbtnconsultarpdf" class="btn btn-sm btn-primary" onclick="formPage.ConsultarPDF();">Auditar PDF</button>
                                    <button type="button" id="formbtnconsultar" class="btn btn-sm btn-primary" onclick="formPage.Concultar();">Consultar</button>
                                    <button type="button" id="formbtnclean" class="btn btn-sm btn-warning" onclick="formPage.clean();">Limpiar</button>


                                </div>




                            </fieldset>
                        </form>
                    </div>
             

            </div>
        </div>
        <!-- end panel -->
    </div>
    <!-- end col-12 -->
</div>
<!-- end row -->
<!-- begin row Datatable -->
<div class="row">
    <!-- begin col-12 -->
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">

                    <a data-click="panel-reload" class="btn btn-xs btn-icon btn-circle btn-success" href="javascript:;"><i class="fa fa-repeat"></i></a>
                    <a data-click="panel-collapse" class="btn btn-xs btn-icon btn-circle btn-warning" href="javascript:;"><i class="fa fa-minus"></i></a>

                </div>
                <h4 class="panel-title">Listado de Pagos Disponible<span class="loader-min-bar"></span></h4>
            </div>
            <div class="panel-body">

                <div id="datatable">
                    @Html.Raw(ViewBag.DataTable)
                </div>

            </div>
        </div>
        <!-- end panel -->
    </div>
    <!-- end col-12 -->
</div>
<!-- end row -->
<!-- #modal-dialog  -->
<div class="modal fade" id="modal-delete">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Eliminar Esquema</h4>
            </div>
            <div class="modal-body">
                Se eliminara el Esquema seleccionado, ¿esta seguro de proceder?
            </div>
            <div class="modal-footer">
                <a href="javascript:;" class="btn btn-sm btn-white" data-dismiss="modal">Cancelar</a>
                <a href="javascript:formPage.delete(true);" class="btn btn-sm btn-danger">Eliminar</a>
            </div>
        </div>
    </div>
</div>


<!-- #modal-dialog -->
<div class="modal " id="modal-publicar-despublicar">
    <div class="modal-dialog">
        <div class="modal-content" style="width:850px;">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" id="btncerrar" onclick="formPage.Btncerrar();" aria-hidden="true">×</button>
                <h4 class="modal-title">Publicar/Despublicar</h4>
            </div>
            <div class="modal-body">


                <div class="row">

                    <div class="col-md-2">
                        <div class="form-group">
                            <label for="id_siuRC">IDSIU</label>
                            <input type="text" id="id_siuRC" class="form-control" disabled="disabled" />
                        </div>
                    </div>

                    <div class="col-md-4">
                        <div class="form-group">
                            <label for="id_siuRC">Nombre</label>
                            <input type="text" id="Nombre" class="form-control" disabled="disabled" />
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="form-group">
                            <label for="concepto">Concepto</label>
                            <input type="text" id="concepto" class="form-control" disabled="disabled" />
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="form-group">
                            <label for="fpago">Fecha de Pago</label>
                            <input type="text" id="fpago" class="form-control" disabled="disabled" />
                        </div>
                    </div>

                </div><br />



                <div class="modal-footer">
                    <input type="hidden" id="idECP" />
                    <button type="button" id="formbtn_publicar_modal" class="btn btn-sm btn-primary" onclick="formPage.btnpublicarModal()" style="display: inline-block;">Publicar</button>
                    <button type="button" id="formbtn_despublicar_model" class="btn btn-sm btn-primary" onclick="formPage.btndespublicarModal();" style="display: inline-block;">DesPublicar</button>
                </div>


            </div>
            <!-- <div class="modal-footer">
                 <a class="btn btn-sm btn-primary" href="javascript:formPage.save();">Grabar</a>
                 <a class="btn btn-sm btn-white" href="javascript:;" data-dismiss="modal">Cancelar</a>
             </div>-->
        </div>
    </div>
</div>




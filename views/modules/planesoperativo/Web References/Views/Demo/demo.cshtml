
@{
    ViewBag.Title = "Start";
}
<!-- begin row Formulario -->
<div class="row" id="frm-bancos">
    <!-- begin col-12 -->
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">

                    <a data-click="panel-collapse" class="btn btn-xs btn-icon btn-circle btn-warning" href="javascript:;"><i class="fa fa-minus"></i></a>

                </div>
                <h4 class="panel-title">Demo</h4>
            </div>

            <div class="panel-body">
                <form action="/" method="POST">
                    <fieldset>
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label for="role">Ciclos *</label>
                                    <select id="ciclos" class="form-control"></select>
                                    
                                   
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label for="descripcion">Periodos</label>
                                    <select id="periodos" class="form-control"></select>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label for="descripcion">Niveles</label>
                                    <select id="niveles" class="form-control"></select>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label for="descripcion">Realiza transferencias bancarias *</label>
                                    <select id="Transferencias" style="width:70px;" class="form-control">
                                        <option value="No">No</option>
                                        <option value="Si">Si</option>
                                    </select>
                                </div>
                            </div>
                        </div>

                        <div class="row right">
                            <button type="button" id="formbtnadd" class="btn btn-sm btn-primary" onclick="formPage.add();">Agregar</button>
                            <button type="button" id="formbtnsave" class="btn btn-sm btn-success" disabled onclick="formPage.save();">Guardar</button>
                            <button type="button" id="formbtndelete" class="btn btn-sm btn-danger" disabled onclick="formPage.delete(false);">Borrar</button>
                            <button type="button" id="formbtnclean" class="btn btn-sm btn-warning" onclick="formPage.clean();">Limpiar</button>
                        </div>
                    </fieldset>
                </form>



            </div>
        </div>
        <!-- end panel -->
    </div>
    <!-- end col-12 -->
</div>

<!-- #modal-dialog CONFIRM DELETE BANCO -->
<div class="modal fade" id="modal-delete-banco">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Eliminar Banco</h4>
            </div>
            <div class="modal-body">
                Se eliminara el banco seleccionado, ¿esta seguro de proceder?
            </div>
            <div class="modal-footer">
                <a href="javascript:;" class="btn btn-sm btn-white" data-dismiss="modal">Cancelar</a>
                <a href="javascript:formPage.delete(true);" class="btn btn-sm btn-danger">Eliminar</a>
            </div>
        </div>
    </div>
</div>


@{
    // ________________________________ ESTADO DE CUENTA WEB ________________________________


    Layout = "~/Views/Shared/_Layout_login_profesor.cshtml";
    ViewBag.Title = "Start";

}

<!-- begin #sidebar MENU -->
<div id="sidebar" class="sidebar">
    <!-- begin sidebar scrollbar -->
    <div data-scrollbar="true" data-height="100%">

        <ul class="nav">
            <li class="nav-profile">

                <div class="info">
                    Pago Profesores
                    <small>@Html.Raw(ViewBag.Profesor)</small>
                </div>
            </li>
        </ul>

        <!-- begin sidebar nav -->

       <ul class="nav">
            <li><a href="/EstadodeCuentaWeb/Home"><i class="fa fa-bank"></i><span>Estado de Cuenta</span></a></li>
            <li><a href="/EstadodeCuentaWeb/Perfil"><i class="fa fa-user"></i><span>Mi Perfil</span></a></li>
            <li><a href="/EstadodeCuentaWeb/PagosPendientes"><i class="fa fa-usd"></i><span>Pagos Pendientes</span></a></li>
            <li><a href="/EstadodeCuentaWeb/PagosDepositados"><i class="fa fa-money"></i><span>Pagos Depositados</span></a></li>
            <li><a href="/EstadodeCuentaWeb/Contratos"><i class="fa fa-file-text"></i><span>Contratos</span></a></li>
            <li class="active"><a href="/EstadodeCuentaWeb/ConstanciasRetencion"><i class="fa fa-file-text-o"></i><span>Constancias de Retención</span></a></li>
            <li><a href="~/Login/Close"><i class="fa fa-sign-out"></i><span>Salir</span></a></li>
            <li><a href="javascript:;" class="sidebar-minify-btn" data-click="sidebar-minify"><i class="fa fa-angle-double-left"></i></a></li>
        </ul>


        <!-- end sidebar nav -->
    </div>
    <!-- end sidebar scrollbar -->
</div>
<div class="sidebar-bg"></div>
<!-- end #sidebar -->
<!-- begin #content -->
<div id="content" class="content">



    <div class="col-md-13">


        <ul class="nav nav-tabs nav-tabs-inverse nav-justified nav-justified-mobile" style="padding-top:0 !important; background:none !important;" data-sortable-id="index-2">
            <li class="active"><a href="#latest-post" data-toggle="tab"> <span class="hidden-xs">Mensuales</span></a></li>
            <li class=""><a href="/EstadodeCuentaWeb/ConstanciasRetencionAnual" > <span class="">Anuales</span></a></li>

        </ul>

        <div class="tab-content" data-sortable-id="index-3">
            <div class="tab-pane fade active in" id="latest-post">
                <div class="height-sm" data-scrollbar="true">



                    <!-- begin breadcrumb
                    <ol class="breadcrumb hidden-print pull-right">
                        <li class="active">Mensuales</li>
                        <li><a href="/EstadodeCuentaWeb/ConstanciasRetencionAnual">Anuales</a></li>
                    </ol> -->
                    <!-- end page-header -->
                    <!-- begin invoice -->
                    <div class="invoice">
                        <div class="invoice-company">
                            <span class="pull-right hidden-print">




                            </span>
                            Constancias de Retención Mensuales
                        </div>



                        <div class="row">



                            <div class="col-sm-6">
                                <label>
                                    Año Escolar
                                    <select id="Anio" class="form-control" aria-controls="data-table" onchange="filter();"></select>
                                </label>
                                <label>
                                    Mes
                                    <select id="Mes" class="form-control" aria-controls="data-table" onchange="filter();">
									<option value="">
									</option><option value="1">Enero</option>
									<option value="2">Febrero</option>
									<option value="3">Marzo</option>
									<option value="4">Abril</option>
									<option value="5">Mayo</option>
									<option value="6">Junio</option>
									<option value="7">Julio</option>
									<option value="8">Agosto</option>
									<option value="9">Septiembre</option>
									<option value="10">Octubre</option>
									<option value="11">Noviembre</option>
									<option value="12">Diciembre</option></select>
                                </label>
                            </div>

                        </div>

                        <div class="table-responsive">
                            <table class="table table-invoice">
                                <thead>
                                    <tr>
                                        <th style="color:#000">UNI</th>
                                        <th style="color:#000">PERIODO</th>
                                        <th style="color:#000">FORMATO</th>
                                        <th style="color:#000">AÑO</th>
                                        <th style="color:#000">MES</th>
                                        <th style="color:#000">PAGOS</th>
                                        <th style="color:#000">MONTO</th>
                                        <th style="color:#000">IVA</th>
                                        <th style="color:#000">IVA RET</th>
                                        <th style="color:#000">ISR RET</th>
                                        <th style="color:#000">DEPOSITO</th>
                                        <th style="color:#000">TIPO DE PAGO</th>
                                    </tr>
                                </thead>
                                <tbody id="ConstanciasRetencionMensual">


                                    @Html.Raw(ViewBag.ConstanciasRetencionMensual)
                                </tbody>
                            </table>


                        </div>







                        <!-- end invoice -->
                    </div>


                </div>
            </div>

        </div>



    </div>


            </div><!--content end-->

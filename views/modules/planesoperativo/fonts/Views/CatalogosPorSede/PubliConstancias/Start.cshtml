
@{
    ViewBag.Title = "Start";
}

<div class="row" id="frm-datospersonas">
    <ol class="breadcrumb pull-right">
        <li><a href="#"></a></li>
        <li><a href="#"></a></li>
    </ol>
</div>
<!-- begin row -->
<div class="row">
    <!-- begin col-12 -->
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a data-click="panel-collapse" class="btn btn-xs btn-icon btn-circle btn-warning" href="javascript:;"><i class="fa fa-minus"></i></a>
                </div>
                <h4 class="panel-title">Publicación Constancias de Retención</h4>
            </div>
            <div class="panel-body">
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#default-tab-1" data-toggle="tab" aria-expanded="true">Retenciones Mensuales</a></li>
                    <li class=""><a href="#default-tab-2" data-toggle="tab" aria-expanded="false">Retenciones Anuales</a></li>
                  
                 
                </ul>
                <form action="/" method="POST">
                    <fieldset>
                        <div class="tab-content" id="tabs">
                            <!-- TAB RETENCION MENSUAL -->
                            <div class="tab-pane fade active in" id="default-tab-1">
                                <div class="row">                             


                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label for="anios">Año *</label>
                                            <select id="anios" class="form-control" >
                                            
                                            </select>
                                        </div>
                                    </div>


                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label for="anios"> Mes *</label>
                                            <select id="Mes" class="form-control" aria-controls="data-table" onchange="filter();">
                                           <option value="">
									</option><option value="1">Enero</option>
									<option value="2">Febrero</option>
									<option value="3">Marzo</option>
									<option value="4">Abril</option>
									<option value="5">Mayo</option>
									<option value="6">Junio</option>
									<option value="7">Julio</option>
									<option value="8">Agosto</option>
									<option value="9">Septiembre</option>
									<option value="10">Octubre</option>
									<option value="11">Noviembre</option>
									<option value="12">Diciembre</option></select>

                                        </div>
                                    </div>



                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label for="fecha_publicacion">Fecha Publicación *</label>
                                            <div class="input-group">

                                                <input type="hidden" class="form-control" id="idPConsMensual">

                                                <input type="text" class="form-control" id="fecha_publicacion">
                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                            </div>
                                        </div>
                                    </div>


                                    <br /><br /><br /><br /><br />
                               

                                    <div class="row right">
                                        <button type="button" id="formbtnadd" class="btn btn-sm btn-primary" onclick="formPage.add();">Agregar</button>
                                                          
                                       <!-- <button type="button" id="formbtnclean" class="btn btn-sm btn-warning" onclick="formPage.clean();">Limpiar</button>-->
                                    </div>


                                </div>
                            </div>
                            <!-- FIN TAB RETENCION MENSUAL -->


                            <!-- TAB RETENCION ANUAL -->
                            <div class="tab-pane fade" id="default-tab-2">
                                <div class="row">
                                  


                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label for="anios_A">Año *</label>
                                            <select id="anios_A" class="form-control"></select>
                                        </div>
                                    </div>



                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label for="fecha_publicacion_A">Fecha Publicación *</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" id="fecha_publicacion_A">
                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                            </div>
                                        </div>
                                    </div>

                                    <br /><br /><br /><br /><br />

                                    

                                    <div class="row right">
                                        <button type="button" id="formbtnadd2" class="btn btn-sm btn-primary" onclick="formPage.add_A();">Agregar</button>
                                       <!-- <button type="button" id="formbtnsave2" class="btn btn-sm btn-primary" disabled onclick="formPage2.save();">Guardar</button>-->
                                      
                                       <!-- <button type="button" id="formbtnclean2" class="btn btn-sm btn-warning" onclick="formPage2.clean();">Limpiar</button>-->
                                    </div>



                                </div>

                            </div>
                            <!-- FIN TAB RETENCION ANUAL -->
                     
                           

                        </div>
                    </fieldset>
                </form>
            </div>
        </div>
        <!-- end panel -->
    </div>
    <!-- end col-12 -->


</div>
<!-- end row -->
<!-- #modal-dialog CONFIRM DELETE -->
<div class="modal fade" id="modal-delete-datospersonas">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Eliminar Persona</h4>
            </div>
            <div class="modal-body">
                Se eliminara la Persona seleccionada, ¿esta seguro de proceder?
            </div>
            <div class="modal-footer">
                <a href="javascript:;" class="btn btn-sm btn-white" data-dismiss="modal">Cancelar</a>
                <a href="javascript:formPage.delete(true);" class="btn btn-sm btn-danger">Eliminar</a>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modal-delete-pensionado">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Eliminar Beneficiario</h4>
            </div>
            <div class="modal-body">
                Se eliminara el beneficiario seleccionado, ¿esta seguro de proceder?
            </div>
            <div class="modal-footer">
                <a href="javascript:;" class="btn btn-sm btn-white" data-dismiss="modal">Cancelar</a>
                <a href="javascript:formPagePensionados.delete(true);" class="btn btn-sm btn-danger">Eliminar</a>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="modal-alert">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Atenci&oacute;n</h4>
            </div>
            <div class="modal-body">
                La suma de los porcentajes debe ser menor a 100%.
            </div>
            <div class="modal-footer">
                <a href="javascript:;" class="btn btn-sm btn-white" data-dismiss="modal">Cerrar</a>
            </div>
        </div>
    </div>
</div>


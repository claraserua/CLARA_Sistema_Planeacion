
<!-- begin row Consulta de Nomina PA -->
<div class="row" id="frm-consulta">
    <!-- begin col-12 -->
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a data-click="panel-collapse" class="btn btn-xs btn-icon btn-circle btn-warning" href="javascript:;"><i class="fa fa-minus"></i></a>
                </div>
                <h4 class="panel-title">Consulta de Pago de la PA</h4>
            </div>

            <div class="panel-body">
                <div id="wizard" class="bwizard clearfix">
                    <ol class="bwizard-steps clearfix" role="tablist">
                        <li role="tab" aria-selected="false" style="z-index: 4;">
                            <span class="label badge-inverse">1</span><span class="hidden-phone">
                                Programación Academica
                            </span><span class="hidden-phone"><small>Importar PA (Banner).</small></span><span class="hidden-phone">
                            </span>
                        </li>
                        <li  id="tab-esquema" role="tab" aria-selected=""  class="active" style="z-index: 3;">
                            <span class="label">2</span><span class="hidden-phone">
                                Esquema de Pagos
                            </span><span class="hidden-phone"><small>Asignar Esquema de Pago (PA).</small></span><span class="hidden-phone">
                            </span>
                        </li>
                        <li id="tab-costos" class="" role="tab" aria-selected="false" style="z-index: 2;">
                            <span class="label">3</span><span class="hidden-phone">
                                Centro de Costos
                            </span><span class="hidden-phone"><small>Asignar Centro de Costos (PA).</small></span><span class="hidden-phone">
                            </span>
                        </li>
                        <li  id="tab-calculo-nomina" role="tab" aria-selected="false" style="z-index: 1;">
                            <span class="label">4</span><span class="hidden-phone">
                                Pago
                            </span><span class="hidden-phone"><small>Calcular Pago (PA).</small></span><span class="hidden-phone">
                            </span>
                        </li>
                    </ol>
                    <!-- begin wizard step-1 -->
                    <!-- end wizard step-1 -->
                    <div class="well">
                        <div class="wizard-step-1 bwizard-activated" id="step1" role="tabpanel" aria-hidden="false">
                            <div class="panel-body">
                                <form action="/" method="POST">
                                    <fieldset>
                                        <legend class="pull-left width-full small"><span id="titulo-legend">Utilice este procedimiento para asignar un Esquema de Pago a la programación academica importada. <a href="javascript:void(0)" onclick="ayuda('paesquema.html')">Más ayuda.</a></span></legend>
                                        <div class="row">
                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label for="Ciclo">Año Escolar</label>
                                                    <select id="Ciclo" class="form-control"></select>
                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label for="Periodo">Periodo</label>
                                                    <select id="Periodo" class="form-control"></select>
                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label for="PartePeriodo">Parte del periodo</label>
                                                    <select id="PartePeriodo" class="form-control"></select>
                                                </div>
                                            </div>
                                            @*<div class="col-md-2">
                                                <div class="form-group">
                                                    <label for="Nivel">Nivel</label>
                                                    <select id="Nivel" class="form-control"></select>
                                                </div>
                                            </div>*@
                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label for="Escuela">Escuela</label>
                                                    <select id="Escuela" class="form-control"></select>
                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label for="TipoPago">Tipos de pago</label>
                                                    <select id="TipoPago" class="form-control"></select>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label for="CampusPA">Campus</label>
                                                    <select id="CampusPA" class="form-control">@Html.Raw(ViewBag.COMBO_CAMPUSPA)</select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row right">
                                            <button type="button" id="formbtnconsultar" class="btn btn-sm btn-primary" onclick="formPage.Consultar();">Consultar</button>
                                            <button type="button" id="formbtntabulador" class="btn btn-sm btn-primary" onclick="formPage.ModalCambiarTabulador();">Tabulador</button>
                                            <button type="button" id="formbtnasignar" class="btn btn-sm btn-primary"  onclick="formPage.ModalAsignarEsquema();">Asignar Esquema</button>
                                            <button type="button" id="formbtnasignarCentroC" class="btn btn-sm btn-primary" onclick="formPage.ModalAsignarCentroCostos();">Asignar Centro Costos</button>
                                            <button type="button" id="formbtnGenerarCentroC" class="btn btn-sm btn-primary" onclick="formPage.GenerarCC();">Generar CC en automático</button>
                                            <button type="button" id="formbtnaCalcularNomina" class="btn btn-sm btn-primary" onclick="formPage.insertaNomina();">Calcular Pago</button>
                                            <!--<button type="button" id="formbtnaCalcularNomina" class="btn btn-sm btn-primary" onclick="formPage.ModalCalcularNomina();">Calcular Nomina</button>insertaNomina-->
                                            <button type="button" id="formbtnclean" class="btn btn-sm btn-warning" onclick="formPage.clean();">Limpiar</button>
                                        </div>
                                    </fieldset>
                                </form>
                            </div>
                            <!-- BEGIN -->
                        </div>
                    </div>
                    <ul class="pager bwizard-buttons">
                        <li  id="btn-ant" class="previous" role="button" aria-disabled="true"><a  id="linkant"  href="javascript:formPage.BotonAntPA()">← Anterior</a></li>
                        <li id="btn-sgt-costos" class="next" role="button" aria-disabled="false" style="display:none"><a id="linksgt" href="javascript:formPage.BotonSgtAsignarCostos()">Siguiente →</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- end panel -->
    </div>
    <!-- end col-12 -->
</div>
<!-- end row -->

<!-- begin row Datatable -->
<div class="row" id="frm-listadoNomina">
    <!-- begin col-12 -->
    <div class="col-md-12 ui-sortable">
        <!-- begin panel -->
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a data-click="panel-reload" class="btn btn-xs btn-icon btn-circle btn-success" href="javascript:;"><i class="fa fa-repeat"></i></a>
                    <a data-click="panel-collapse" class="btn btn-xs btn-icon btn-circle btn-warning" href="javascript:;"><i class="fa fa-minus"></i></a>
                </div>
                <h4 class="panel-title">Listado de Nomina Disponible <span class="loader-min-bar"></span></h4>
            </div>
            <div class="panel-body">
                <div id="datatable">
                    @Html.Raw(ViewBag.DataTable)
                </div>
            </div>
        </div>
        <!-- end panel -->
    </div>
    <!-- end col-12 -->
</div>
<!-- end row -->

